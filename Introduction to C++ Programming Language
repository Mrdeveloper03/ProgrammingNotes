  <script>
  // script.js
function compileCode() {
    const code = document.getElementById('code').value;
    const outputElement = document.getElementById('output');

    // For demonstration purposes, we'll simulate the compilation process
    // In a real-world scenario, you would send the code to a server to compile and run it
    if (code.includes('cout')) {
        outputElement.textContent = 'Hello, World!';
    } else {
        outputElement.textContent = 'Compilation Error: Please include a valid C++ code.';
    }
}

  </script>
